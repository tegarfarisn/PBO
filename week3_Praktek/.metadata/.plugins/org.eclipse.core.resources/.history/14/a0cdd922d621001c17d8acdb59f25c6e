		System.out.println("Direktur 1  : "+ getDirektur());
		System.out.println("Dosen 1     : "+ getDosen());
		System.out.println("Mahasiswa 1 : "+ getMahasiswa());
		System.out.println("Staff 1     : "+ getStaff());
		System.out.println("Security 1  : "+ getSecurity());
	}
